pragma solidity ^0.8.0;

import "hardhat/console.sol";

contract getFeedback {
    address sender;
    uint responses;

    constructor() {
        sender=msg.sender;
        responses=0
    }

    function response() public viewable returns(uint) {
        if(msg.value>0) {
            responses=responses+1;
        }
        return getNumberofResponses() public view returns(uint) {
            return responses;
        }

        function getAllResponses() public {
            if(msg.sender==address)
            push(viewable(address));
        }
    }
}
